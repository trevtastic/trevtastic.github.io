<!DOCTYPE html>
<html lang="en" class="s-sr t-bluegrey" data-ui="{{ browser }}" data-ua="{{ useragent }}">
<head>
	<meta charset="utf-8">
	<meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
	<meta name="viewport" content="width=device-width,initial-scale=1">
	<title>Trevor Chigonda | {{ title }}</title>
	<meta name="description" content="Welcome to MERN.">
	<meta name="keywords" content="web development, software development, software engineering">
	<meta name="msapplication-config" content="/browserconfig.xml">
	<meta name="theme-color" content="#000000">
	<meta name="apple-mobile-web-app-capable" content="yes">
	<meta name="apple-mobile-web-app-status-bar" content="#000000">
	<meta name="apple-mobile-web-app-status-bar-style" content="black">
	<meta name="apple-mobile-web-app-title" content="MERN App">
	<meta property="og:site_name" content="MERN">
	<meta property="og:image_secure" content="https://mern.com/images/content/bg/site-poster.jpg?v=1647625898219">
	<meta property="og:title" content="MERN | Web development">
	<meta property="og:description" content="Web development.">
	<meta property="og:type" content="website">
	<meta property="og:url" content="https://mern.com">
	<link rel="apple-touch-icon" sizes="180x180" href="/images/common/touch/apple-touch-icon.png?v=1647625898219">
	<link rel="apple-touch-startup-image" media="(device-width: 320px) and (device-height: 568px) and (-webkit-device-pixel-ratio: 2)" href="/images/common/touch/apple-launch-640x1096.png?v=1647625898219">
	<link rel="apple-touch-startup-image" media="(device-width: 320px) and (device-height: 568px) and (-webkit-device-pixel-ratio: 2)" href="/images/common/touch/apple-launch-640x1136.png?v=1647625898219">
	<link rel="icon" type="image/png" sizes="180x180" href="/images/common/favicon/favicon-180x180.png?v=1647625898219">
	<link rel="icon" type="image/png" sizes="96x96" href="/images/common/favicon/favicon-96x96.png?v=1647625898219">
	<link rel="icon" type="image/png" sizes="32x32" href="/images/common/favicon/favicon-32x32.png?v=1647625898219">
	<link rel="icon" type="image/png" sizes="16x16" href="/images/common/favicon/favicon-16x16.png?v=1647625898219">
	<link rel="shortcut icon" href="/favicon.ico?v=1647625898219">
	<link rel="manifest" href="/manifest.json?v=1647625898219">
	<link rel="mask-icon" color="#f62b54" href="/images/common/touch/safari-pinned-tab.svg?v=1647625898219">
	<link rel="stylesheet" href="/assets/css/gen/common.80352e4bf6c2eb668e2b.css">
	<script type="text/javascript" src="/assets/js/lib/jquery-3.5.1.js"></script>
</head>
<body>
	{% include 'pulse-loader.njk' %}
	<div id="appMountPoint">
		{% if content_toolbar %}
			<div id="toolbar">
				{% block toolbar %}
					<div class="o-container-fluid">
						<div class="o-row">
							<div class="o-col">
								{% if showCookieDisclosure %}
									{% include 'cookie.njk' %}
								{% endif %}
							</div>
						</div>
					</div>
				{% endblock %}
			</div>
		{% endif %}
		{% if content_head %}
			<div id="header">
				<div id="head" class="{% if head_scroll %}o-head-scroll{% endif %}">
					{% block head %}
						{% include 'navigation.njk' %}
					{% endblock %}
				</div>
				<div id="head-after"></div>
				<div id="showcase">
					{% block showcase %}{% endblock %}
				</div>
			</div>
		{% endif %}
		{% if content_utility %}
			<div id="utility">
				{% block utility %}{% endblock %}
			</div>
		{% endif %}
		{% if content_top %}
			<div id="top">
				{% block top %}{% endblock %}
			</div>
		{% endif %}
		{% if content_main %}
			<div id="main">
				{% if content_mainTop %}
					<div id="main-top">
						{% block mainTop %}{% endblock %}
					</div>
				{% endif %}
				<div id="main-content">
					<div class="o-container-fluid">
						<div class="o-row">
							{% if content_sidebarA %}
								<div id="sidebar-a">
									{% block sidebarA %}{% endblock %}
								</div>
							{% endif %}
							<div id="content-center">
								{% block contentCenter %}{% endblock %}
							</div>
							{% if content_sidebarB %}
								<div id="sidebar-b">
									{% block sidebarB %}{% endblock %}
								</div>
							{% endif %}
						</div>
					</div>
				</div>
				{% if content_mainBottom %}
					<div id="main-bottom">
						{% block mainBottom %}{% endblock %}
					</div>
				{% endif %}
			</div>
		{% endif %}
		{% if content_bottom %}
			<div id="bottom">
				{% block bottom %}{% endblock %}
			</div>
		{% endif %}
		{% if content_tray %}
			<div id="tray">
				{% block tray %}{% endblock %}
			</div>
		{% endif %}
		{% if content_footer %}
			<div id="footer" class="u-bg-foreground">
				{% block footer %}
					{% include 'footer.njk' %}
					{% include 'copyright.njk' %}
				{% endblock %}
			</div>
			<div id="footer-after"></div>
		{% endif %}
	</div>
	<div>
		<script type="text/javascript" src="/assets/js/gen/vendor.59054062b651152d5c3d.js"></script>
		<script type="text/javascript" src="/assets/js/gen/common.80352e4bf6c2eb668e2b.js"></script>
	</div>
</body>
</html>